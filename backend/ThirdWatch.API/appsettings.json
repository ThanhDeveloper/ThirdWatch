{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Sql": "<KeyVault>"
  },
  "Jwt": {
    "Secret": "your-super-secret-key-with-at-least-32-characters",
    "Issuer": "ThirdWatch",
    "Audience": "ThirdWatchUsers",
    "ExpiryInHours": 1
  },
  "ApplyMigrationsOnStartup": false
}
